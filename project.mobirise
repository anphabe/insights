\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    \n    <div mbr-class=\"{'container': !fullWidth}\" class=\"align-center\"> \n        <div class=\"back\"></div>\n        <div class=\"row justify-content-center\">\n            <div class=\"box col-lg-9 col-12\">\n                    <img src=\"@PROJECT_PATH@/assets/images/inochi.jpg\" alt=\"Voucher Inochi ưu đãi 40%\">\n            </div>            \n            <div class=\"align-self-top col-lg-3 col-12\">                                     \n                <div class=\"card-wrap\">\n                    <h6 class=\"card-title mbr-fonts-style mb-4\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".card-title\"><b>Voucher Inochi ưu đãi 40%</b></h6>\n                    <p class=\"mbr-text mbr-fonts-style mb-4\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">Áp dụng cho đơn hàng từ 500.000VNĐ. <a href=\"thongtinquatang.html#inochi\" class=\"text-primary\">Chi tiết ➝</a></p>\n                    <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn mt-3 fade-in-up\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-primary\" href=\"https://insights-w2.anphabe.com/\" data-app-placeholder=\"Type Text\">NHẬN VOUCHER ➝</a></div>\n                </div>\n            </div>\n            \n        </div>\n    </div>\n</section>",
